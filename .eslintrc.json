{
  "env": {
    "browser": true,
    "es2020": true,
    "jest": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "implicit-arrow-linebreak": "off",
    "no-confusing-arrow": "off"
  }
}
