{
  "env": {
    "browser": true,
    "es2020": true,
    "jest": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "semi": "off",
    "arrow-parens": "off",
    "implicit-arrow-linebreak": "off",
    "no-confusing-arrow": "off"
  }
}
